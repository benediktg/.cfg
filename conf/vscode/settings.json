{
    "[jsx-tags]": {
        "editor.tabSize": 2
    },
    "auto-rename-tag.activationOnLanguage": [
        "blade",
        "ejs",
        "handlebars",
        "html",
        "javascript",
        "javascriptreact",
        "jinja",
        "liquid",
        "markdown",
        "php",
        "plaintext",
        "razor",
        "typescriptreact",
        "vue",
        "xml"
    ],
    "closeTag.ignoreTags": "html",
    "deploy": {
        "disableNewVersionPopups": true,
        "displayLoadedPlugins": false,
        "displayNetworkInfo": false,
        "hideDeployResultInStatusBarAfter": 2,
        "ignore": [
            ".vscode/**"
        ],
        "openOutputOnDeploy": false,
        "showDeployResultInStatusBar": true,
        "showPopupOnSuccess": false,
        "showWarningIfIgnored": false,
        "values": [
            {
                "name": "SSH_AUTH_SOCK",
                "type": "env"
            }
        ]
    },
    "editor.autoIndent": true,
    "editor.cursorBlinking": "solid",
    "editor.fontFamily": "'Fira Code', Hack, monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.formatOnPaste": true,
    "editor.minimap.enabled": false,
    "editor.minimap.showSlider": "always",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [
        100
    ],
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 120,
    "editor.wrappingIndent": "indent",
    "eslint.packageManager": "yarn",
    "extensions.autoUpdate": true,
    "files.associations": {
        "**/*_vars/**/*.yml": "ansible",
        "**/*ansible*/**/*.yml": "ansible",
        "**/defautls/**/*": "ansible",
        "**/handler/*.yml": "ansible",
        "**/inventory/*/*": "ansible",
        "**/playbooks/**/*.yml": "ansible",
        "**/roles/**/*.yml": "ansible",
        "**/tasks/**/*.yml": "ansible",
        "**/vars/**/*.yml": "ansible"
    },
    "files.autoSave": "onWindowChange",
    "files.exclude": {
        "**/*.min.css": true,
        "**/*.min.js": true,
        "**/*.min.map": true,
        "**/*.mo": true,
        "**/*.pyc": true,
        "**/*.pyo": true,
        "**/*~": true,
        "**/.DS_Store": true,
        "**/.coverage": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.idea": true,
        "**/.project": true,
        "**/.pydevproject": true,
        "**/.svn": true,
        "**/.tox": true,
        "**/CVS": true,
        "**/__pycache__": true,
        "**/db.sqlite3": true,
        ".vscode/tags": true
    },
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "gitProjectManager.baseProjectsFolders": [
        "~/Projekte"
    ],
    "gitProjectManager.ignoredFolders": [
        "node_modules"
    ],
    "gitProjectManager.maxDepthRecursion": 3,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressUpdateNotice": true,
        "suppressWelcomeNotice": true
    },
    "gitlens.codeLens.enabled": false,
    "gitlens.keymap": "chorded",
    "html.format.endWithNewline": true,
    "html.format.indentHandlebars": true,
    "html.format.maxPreserveNewLines": 3,
    "html.format.wrapLineLength": 100,
    "html.suggest.angular1": false,
    "html.suggest.ionic": false,
    "javascript.implicitProjectConfig.checkJs": true,
    "javascript.referencesCodeLens.enabled": true,
    "npm.packageManager": "yarn",
    "prettier.bracketSpacing": false,
    "prettier.eslintIntegration": true,
    "prettier.printWidth": 100,
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "es5",
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Path": "/usr/bin/flake8",
    "python.linting.pylintEnabled": false,
    "python.venvPath": "~/.venvs",
    "search.useIgnoreFiles": true,
    "search.useIgnoreFilesByDefault": true,
    "shellcheck.run": "onSave",
    "svgviewer.enableautopreview": true,

{
    "[jsx-tags]": {
        "editor.tabSize": 2
    },
    "auto-rename-tag.activationOnLanguage": [
        "blade",
        "ejs",
        "handlebars",
        "html",
        "javascript",
        "javascriptreact",
        "jinja",
        "liquid",
        "markdown",
        "php",
        "plaintext",
        "razor",
        "typescriptreact",
        "vue",
        "xml"
    ],
    "cdnjs.quoteStyle": "double",
    "closeTag.ignoreTags": "html",
    "diffEditor.ignoreTrimWhitespace": true,
    "editor.autoIndent": true,
    "editor.cursorBlinking": "solid",
    "editor.fontFamily": "'Fira Code', Hack, monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "editor.minimap.showSlider": "always",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [
        100
    ],
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 120,
    "editor.wrappingIndent": "indent",
    "emmet.includeLanguages": {
        "django-html": "html",
        "javascript": "javascriptreact",
        "vue-html": "html"
    },
    "eslint.autoFixOnSave": true,
    "eslint.packageManager": "yarn",
    "explorer.openEditors.visible": 0,
    "extensions.autoUpdate": true,
    "files.associations": {
        "**/*_vars/**/*.yml": "ansible",
        "**/*ansible*/**/*.yml": "ansible",
        "**/defautls/**/*": "ansible",
        "**/handler/*.yml": "ansible",
        "**/inventory/*/*": "ansible",
        "**/playbooks/**/*.yml": "ansible",
        "**/roles/**/*.yml": "ansible",
        "**/tasks/**/*.yml": "ansible",
        "**/vars/**/*.yml": "ansible"
    },
    "files.autoSave": "onFocusChange",
    "files.exclude": {
        "**/*.min.css": true,
        "**/*.min.js": true,
        "**/*.min.map": true,
        "**/*.mo": true,
        "**/*.pyc": true,
        "**/*.pyo": true,
        "**/*~": true,
        "**/.DS_Store": true,
        "**/.coverage": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.idea": true,
        "**/.project": true,
        "**/.pydevproject": true,
        "**/.svn": true,
        "**/.tox": true,
        "**/CVS": true,
        "**/__pycache__": true,
        "**/db.sqlite3": true,
        ".vscode/tags": true
    },
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "gitProjectManager.baseProjectsFolders": [
        "~/Projekte"
    ],
    "gitProjectManager.ignoredFolders": [
        "node_modules"
    ],
    "gitProjectManager.maxDepthRecursion": 3,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true,
        "suppressUpdateNotice": true,
        "suppressWelcomeNotice": true
    },
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.codeLens.recentChange.enabled": false,
    "gitlens.codeLens.scopes": [
        "document"
    ],
    "gitlens.currentLine.enabled": false,
    "gitlens.keymap": "chorded",
    "html.format.endWithNewline": true,
    "html.format.indentHandlebars": true,
    "html.format.maxPreserveNewLines": 3,
    "html.format.wrapLineLength": 100,
    "html.suggest.angular1": false,
    "html.suggest.ionic": false,
    "javascript.implicitProjectConfig.checkJs": true,
    "javascript.referencesCodeLens.enabled": true,
    "npm.packageManager": "yarn",
    "prettier.bracketSpacing": false,
    "prettier.disableLanguages": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact",
        "vue"
    ],
    "prettier.eslintIntegration": true,
    "prettier.printWidth": 100,
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "es5",
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Path": "/usr/bin/flake8",
    "python.linting.pylintEnabled": false,
    "python.venvPath": "~/.venvs",
    "search.useIgnoreFiles": true,
    "shellcheck.run": "onSave",
    "svgviewer.enableautopreview": true,
    "telemetry.enableTelemetry": false,
    "todohighlight.include": [
        "**/*.js",
        "**/*.jsx",
        "**/*.ts",
        "**/*.tsx",
        "**/*.html",
        "**/*.php",
        "**/*.css",
        "**/*.scss",
        "**/*.py"
    ],
    "tslint.packageManager": "yarn",
    "typescript.implementationsCodeLens.enabled": true,
    "typescript.referencesCodeLens.enabled": true,
    "window.menuBarVisibility": "toggle",
    "window.openFilesInNewWindow": "default",
    "window.restoreFullscreen": true,
    "window.zoomLevel": 0,
    "workbench.activityBar.visible": true,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.startupEditor": "none"
}